{
	"luci-app-fancontrol": {
		"description": "Fan General Control",
		"read": {
			"uci": [
				"fancontrol"
			],
			"file": {
				"/sys/devices/virtual/thermal/*/*": [
					"read"
				],
				"/sys/class/hwmon/*/pwm*": [
					"read"
				]
			}
		},
		"write": {
			"uci": [
				"fancontrol"
			],
			"file": {
				"/sys/class/hwmon/*/pwm*": [
					"write"
				]
			}
		}
	}
}